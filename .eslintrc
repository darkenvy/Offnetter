{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "off",
      {
        "singleQuote": true,
        "trailingComma": "all",
        "printWidth": 120
      }
    ],
    "no-use-before-define": "off",
    "no-case-declarations": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "prefer-destructuring": "warn",
    "no-plusplus": "off",
    "no-shadow": "off",
    "prefer-rest-params": "off",
    "class-methods-use-this": "off",
    "no-return-assign": "off"
  }
}